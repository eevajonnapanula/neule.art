{
  "name": "neule.art",
  "version": "1.0.0",
  "description": "Neule.art for visualising knitting patterns",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "watch": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "start": "npx @11ty/eleventy --serve",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "build-prod": "npm run build",
    "eslint": "eslint",
    "stylelint": "stylelint **/**.css",
    "lint": "npm run eslint && npm run stylelint",
    "updateStock": "node ./helpers/getColors.js",
    "clean": "rm -rf netlify && npm run build-prod"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/eevajonnapanula/neule.art"
  },
  "author": {
    "name": "Eeva-Jonna Panula",
    "email": "hello@eevis.codes",
    "url": "https://eevis.codes/"
  },
  "license": "MIT",
  "homepage": "https://neule.art",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@double-great/stylelint-a11y": "^2.0.2",
    "@netlify/functions": "^2.0.2",
    "axe-core": "^4.7.2",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "cypress": "^12.14.0",
    "cypress-axe": "^1.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "firebase-admin": "^11.10.1",
    "prettier": "^2.8.8",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "dependencies": {
    "@11ty/eleventy-upgrade-help": "^2.0.5"
  }
}
